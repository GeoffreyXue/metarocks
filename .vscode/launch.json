{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug RocksDB",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/example_parquet/parquet_example",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/example_parquet",
        "environment": [],
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "build"
      }
    ]
  }